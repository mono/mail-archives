<glade-catalog name="gnomedb" library="gladegnomedb" depends="gnome" domain="glade-3" book="libgnomedb">
  <init-function>glade_gnomedb_init</init-function>
  <glade-widget-classes>
    <glade-widget-class name="GnomeDbBasicForm" generic-name="gnome_db_basic_form" title="GnomeDbBasicForm">
      <post-create-function>glade_gnome_db_basic_form_post_create</post-create-function>
      <get-internal-child-function>glade_gnome_basic_form_app_get_internal_child</get-internal-child-function>
      <properties>
         <property id="show-entries-actions" _name="ShowEntriesActions" save="False">
	   <tooltip>Show or hide the actions button available at the end of each data entry in the form</tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_basic_form_set_show_entries_actions</set-function>
	 </property>
         <property id="entries-auto-default" _name="EntriesAutoDefault" save="False">
	   <tooltip>Set default value if a invalid one is assigned to the entries in the form</tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_basic_form_set_entries_auto_default</set-function>
	 </property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbComboBox" generic-name="gnome_db_combobox" title="GnomeDb ComboBox" />
    
    <glade-widget-class name="GnomeDbConnectionProperties" generic-name="gnome_db_connection_properties" title="GnomeDb Connection Properties" />

    <glade-widget-class name="GnomeDbDataImport" generic-name="gnome_db_data_import" title="GnomeDb Data Import" />

    <glade-widget-class name="GnomeDbDataSourceSelector" generic-name="gnome_db_data_source_selector" title="GnomeDb Data Source Selector" />

    <glade-widget-class name="GnomeDbDataWidgetInfo" generic-name="gnome_db_data_widget_info" title="GnomeDb Data Widget Info" />

    <glade-widget-class name="GnomeDbmsUpdateViewer" generic-name="gnome_db_dbms_update_viewer" title="GnomeDb DBMS Update Viewer" />

    <glade-widget-class name="GnomeDbDsnConfigDruid" generic-name="gnome_db_dsn_config_druid" title="GnomeDb DNS Config Druid" />

    <glade-widget-class name="GnomeDbDsnConfig" generic-name="gnome_db_dsn_config" title="GnomeDb DNS Config" />

    <glade-widget-class name="GnomeDbDsnSpec" generic-name="gnome_db_dsn_spec" title="GnomeDb DNS Specifications" />

    <glade-widget-class name="GnomeDbEditor" generic-name="gnome_db_editor" title="GnomeDb Database's Procedures and Functions Editor">
      <properties>
         <property id="editable" _name="Editable" save="True">
	   <tooltip>Allow modify of the procedures and functions stored in the database</tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_editor_set_editable</set-function>
	 </property>
         <property id="enable-highlight" _name="EnableHighlight" save="True">
	   <tooltip>Enable highlight of the code</tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_editor_set_highlight</set-function>
	 </property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbErrorDialog" generic-name="gnome_db_error_dialog" title="GnomeDb Error Dialog">
      <properties>
	<property id="title" _name="Title">
	  <spec>glade_standard_string_spec</spec>
	  <set-function>glade_gnome_db_error_dialog_set_title</set-function>
	</property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbError" generic-name="gnome_db_error" title="GnomeDb Error" />

    <glade-widget-class name="GnomeDbFindDialog" generic-name="gnome_db_find_dialog" title="GnomeDb Find Dialog" />

    <glade-widget-class name="GnomeDbForm" generic-name="gnome_db_form" title="GnomeDb Form" />


    <glade-widget-class name="GnomeDbGrayBar" generic-name="gnome_db_gray_bar" title="GnomeDb Gray Bar">
      <properties>
        <property id="show_icon" disabled="True"/>
        <property id="stock" disabled="True"/>
	<property id="filename" _name="File">
	  <spec>glade_standard_pixbuf_spec</spec>
	  <_tooltip></_tooltip>
	  <set-function>glade_gnome_db_gray_bar_set_filename</set-function>
	</property>
	<property id="scaled-width" _name="Scaled Width">
	  <spec>glade_standard_int_spec</spec>
	  <_tooltip>The width to scale the pixmap to</_tooltip>
	  <set-function>glade_gnome_db_gray_bar_set_scaled_width</set-function>
	</property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbGrid" generic-name="gnome_db_grid" title="GnomeDb Grid">
      <properties>
	<property id="sample-size" _name="Sample Size">
	  <spec>glade_standard_int_spec</spec>
	  <_tooltip>Sets the sample size of the data in the grid</_tooltip>
	  <set-function>glade_gnome_db_grid_set_sample_size</set-function>
	</property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbLoginDialog" generic-name="gnome_db_login_dialog" title="GnomeDb Login Dialog" />

    <glade-widget-class name="GnomeDbLogin" generic-name="gnome_db_login" title="GnomeDb Login Widget">
      <properties>
         <property id="createbutton" _name="Create Button" save="True">
	   <tooltip>Allows to create the Button/tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_login_set_create_button</set-function>
	 </property>
         <property id="showdnsselector" _name="Show DNS Selector" save="True">
	   <tooltip>Show the DNS Selector</tooltip>
	   <spec>glade_standard_boolean_spec</spec>
	   <set-function>glade_gnome_db_login_set_show_dns_selector</set-function>
	 </property>
      </properties>
    </glade-widget-class>

    <glade-widget-class name="GnomeDbProviderSelector" generic-name="gnome_db_provider_selector" title="GnomeDb Provider Selector" />


    <glade-widget-class name="GnomeDbRawForm" generic-name="gnome_db_raw_form" title="GnomeDb Raw Form" />


    <glade-widget-class name="GnomeDbRawGrid" generic-name="gnome_db_raw_grid" title="GnomeDb Raw Grid" />

    <glade-widget-class name="GnomeDbReportEditor" generic-name="gnome_db_report_editor" title="GnomeDb Report Editor" />


    <glade-widget-class name="GnomeDbSelector" generic-name="gnome_db_selector" title="GnomeDb Selector" />


    <glade-widget-class name="GnomeDbSqlConsole" generic-name="gnome_db_sql_console" title="GnomeDb SQL Console" />

  </glade-widget-classes>

</glade-catalog>