<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE language SYSTEM "language.dtd">
<language _name="ChangeLog" version="1.0" _section="Markup" mimetypes="text/x-changelog">
	<pattern-item _name="Date" style = "Others">
		<regex>[0-9]{4}-[0-1][0-9]-[0-3][0-9]</regex>
	</pattern-item>
	<pattern-item _name="Email" style = "Others">
		<regex>&lt;[^@]+@[^&gt;]+&gt;</regex>
	</pattern-item>
	<pattern-item _name="File" style = "Others">
		<regex>\t\* [^:]+:</regex>
	</pattern-item>
</language>
