+using System;
+
+using MonoDevelop.Projects;
+using MonoDevelop.Ide.Gui;
+using MonoDevelop.Components.Commands;
+
+using CBinding.Parser;
+
+namespace CBinding
+{
+	public class StartupHandler : CommandHandler
+	{
+		protected override void Run ()
+		{
+			IdeApp.ProjectOperations.EntryAddedToCombine += EntryAdded; 
+		}
+		
+		protected void EntryAdded (object sender, CombineEntryEventArgs e)
+		{
+			CProject p = e.CombineEntry as CProject;
+			
+			if (p == null)
+				return;
+			
+			foreach (ProjectFile f in p.ProjectFiles) {
+				if (f.BuildAction == BuildAction.Compile) {
+					TagDatabaseManager.Instance.UpdateFileTags (p, f.Name);
+				}
+			}
+		}
+	}
+}
