Index: Extras/CommonTemplates/CommonTemplates.addin.xml
===================================================================
--- Extras/CommonTemplates/CommonTemplates.addin.xml	(revision 0)
+++ Extras/CommonTemplates/CommonTemplates.addin.xml	(revision 0)
@@ -0,0 +1,54 @@
+<AddIn id          = "CommonTemplates"
+       name        = "Common Templates"
+       author      = "Atsushi Enomoto"
+       copyright   = "GPL"
+       url         = "http://www.monodevelop.com/"
+       description = "Common Templates"
+	   category    = "Misc"
+       version     = "0.6">
+	
+	<Runtime>
+		<Import assembly = "CommonTemplates.dll"/>
+	</Runtime>
+
+	<Dependencies>
+		<AddIn id="MonoDevelop.SourceEditor" version="0.9.0"/>
+		<AddIn id="MonoDevelop.Ide" version="0.9.0"/>
+		<AddIn id="MonoDevelop.Projects.Gui" version="0.9.0"/>
+	</Dependencies>
+	
+	<Extension path = "/MonoDevelop/FileTemplates">
+		<FileTemplate id = "EmptyHTMLFile" resource = "EmptyHTMLFile.xft.xml"/>
+		<FileTemplate id = "EmptyCSSFile" resource = "EmptyCSSFile.xft.xml"/>
+		<FileTemplate id = "AppConfigFile" resource = "AppConfigFile.xft.xml"/>
+		<FileTemplate id = "EmptyResourceFile" resource = "EmptyResourceFile.xft.xml"/>
+		<FileTemplate id = "EmptyTextFile" resource = "EmptyTextFile.xft.xml"/>
+		<FileTemplate id = "EmptyXMLFile" resource = "EmptyXMLFile.xft.xml"/>
+	</Extension>
+
+	<Extension path = "/Workspace/Icons">
+		<!-- Resources -->
+		<Icon id         = "ResourceFileIcon"
+		      extensions = ".resources,.resx"
+		      resource   = "md-resource-file-icon" />
+		<!-- HTML -->
+		<Icon id         = "HtmlFileIcon"
+		      extensions = ".html,.htm"
+		      resource   = "md-html-file-icon"/>
+		<!-- XML -->
+		<Icon id         = "XmlFileIcon"
+		      extensions = ".xml"
+		      resource   = "md-file-xml-icon" />
+		<!-- TextFiles -->
+		<Icon id         = "TextFileIcon"
+		      extensions = ".txt,.doc"
+		      resource   = "md-text-file-icon"/>
+
+	</Extension>
+
+	<Extension path = "/SharpDevelop/Workbench/LanguageBindings">
+		<LanguageBinding id    = "CSS"
+		                 supportedextensions = ".css"
+		                 class = "CSharpBinding.CSSLanguageBinding" />
+	</Extension>
+</AddIn>
Index: Extras/CommonTemplates/Makefile.am
===================================================================
--- Extras/CommonTemplates/Makefile.am	(revision 0)
+++ Extras/CommonTemplates/Makefile.am	(revision 0)
@@ -0,0 +1,58 @@
+ADDIN_BUILD = $(top_builddir)/build/AddIns/BackendBindings
+ASSEMBLY = $(ADDIN_BUILD)/CommonTemplates.dll
+
+PARSE = parse.exe
+
+DLLS = /r:System.Drawing.dll \
+	/r:$(top_builddir)/build/bin/MonoDevelop.Core.dll \
+	/r:$(top_builddir)/build/AddIns/MonoDevelop.Core.Gui.dll \
+	/r:$(top_builddir)/build/AddIns/MonoDevelop.SourceEditor.dll \
+	/r:$(top_builddir)/build/AddIns/MonoDevelop.Projects.dll \
+	/r:$(top_builddir)/build/AddIns/MonoDevelop.Ide.dll \
+	/r:$(top_builddir)/build/AddIns/ICSharpCode.SharpRefactory.dll \
+	/r:$(top_builddir)/build/AddIns/MonoDevelop.Components.dll  \
+	$(GTK_SHARP_LIBS) \
+	$(GLADE_SHARP_LIBS)
+
+FILES = \
+CSSLanguageBinding.cs \
+CSSCompilerManager.cs
+
+RES = \
+templates/AppConfigFile.xft.xml \
+templates/EmptyCSSFile.xft.xml \
+templates/EmptyHTMLFile.xft.xml \
+templates/EmptyResourceFile.xft.xml \
+templates/EmptyTextFile.xft.xml \
+templates/EmptyXMLFile.xft.xml
+
+ADDIN = CommonTemplates.addin.xml
+
+all: $(ASSEMBLY) $(ADDIN_BUILD)/$(ADDIN)
+
+$(ADDIN_BUILD)/$(ADDIN): $(srcdir)/$(ADDIN)
+	mkdir -p $(ADDIN_BUILD)
+	cp $(srcdir)/$(ADDIN) $(ADDIN_BUILD)/$(ADDIN)
+
+$(ASSEMBLY): $(build_sources) $(RES)
+	mkdir -p $(ADDIN_BUILD)
+	$(CSC) $(CSC_FLAGS) -out:$@ -target:library $(DLLS) \
+		$(RES:%=/resource:%) $(build_sources)
+
+parse: $(PARSE)
+
+$(PARSE): parse.cs $(ASSEMBLY)
+	$(CSC) -out:$@ parse.cs $(DLLS) -r:$(ASSEMBLY)
+
+run-parse-test: $(PARSE)
+	MONO_PATH=../../../../../build/AddIns/BackendBindings/:../../../../../build/bin/ mono $(PARSE) .
+
+commontemplateslibdir = $(MD_ADDIN_DIR)/BackendBindings
+commontemplateslib_DATA = $(ASSEMBLY) $(ADDIN)
+
+CLEANFILES = $(ASSEMBLY) $(ASSEMBLY).mdb $(PARSE) $(PARSE).mdb
+
+EXTRA_DIST = $(FILES) $(ADDIN) $(RES)
+
+include $(top_srcdir)/Makefile.include
+
Index: Extras/CommonTemplates/CSSLanguageBinding.cs
===================================================================
--- Extras/CommonTemplates/CSSLanguageBinding.cs	(revision 0)
+++ Extras/CommonTemplates/CSSLanguageBinding.cs	(revision 0)
@@ -0,0 +1,62 @@
+// <file>
+//     <copyright see="prj:///doc/copyright.txt"/>
+//     <license see="prj:///doc/license.txt"/>
+//     <owner name="Mike KrÃ¼ger" email="mike@icsharpcode.net"/>
+//     <version value="$version"/>
+// </file>
+
+using System;
+using System.IO;
+using System.Diagnostics;
+using System.Collections;
+using System.Reflection;
+using System.Resources;
+using System.Xml;
+using System.CodeDom.Compiler;
+using System.Threading;
+
+using MonoDevelop.Projects;
+using MonoDevelop.Core;
+
+namespace CSharpBinding
+{
+	public class CSSLanguageBinding : ILanguageBinding
+	{
+		public const string LanguageName = "CSS";
+		
+		CSSCompilerManager   compilerManager  = new CSSCompilerManager();
+		
+		public string Language {
+			get {
+				return LanguageName;
+			}
+		}
+		
+		public bool CanCompile(string fileName)
+		{
+			Debug.Assert(compilerManager != null);
+			return compilerManager.CanCompile(fileName);
+		}
+		
+		public ICompilerResult Compile (ProjectFileCollection projectFiles, ProjectReferenceCollection references, DotNetProjectConfiguration configuration, IProgressMonitor monitor)
+		{
+			Debug.Assert(compilerManager != null);
+			return compilerManager.Compile (projectFiles, references, configuration, monitor);
+		}
+		
+		public void GenerateMakefile (Project project, Combine parentCombine)
+		{
+			//compilerManager.GenerateMakefile (project, parentCombine);
+		}
+		
+		public ICloneable CreateCompilationParameters (XmlElement projectOptions)
+		{
+			return null;//new CSharpCompilerParameters();
+		}
+		
+		public string CommentTag
+		{
+			get { return "//"; }
+		}
+	}
+}
