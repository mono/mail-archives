<Addin namespace = "MonoDevelop">

	<Dependencies>
		<Addin id="Ide" version="0.14.0"/>
	</Dependencies>

	<Runtime>
		<!-- All template files must be imported in this sections -->
		<Import file="HelloTemplate.xpt.xml"/>
	</Runtime>

	<!-- Extensions -->

	<Extension path = "/MonoDevelop/FileTemplates">
	<!--	Register file templates here
		<FileTemplate file = "some template"/> 
	-->
	</Extension>
	
	<Extension path = "/MonoDevelop/ProjectTemplates">
	<!--	Register project templates here -->
		<ProjectTemplate file = "HelloTemplate.xpt.xml"/>
	</Extension>
</Addin>
