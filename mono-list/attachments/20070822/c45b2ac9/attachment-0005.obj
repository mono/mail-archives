<Canvas xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="NDSFlickr.Test;assembly=bin/ndsflickr.dll"
        Loaded="LoadedEvent"
        Width="720" Height="576">

  <Canvas.Resources>
    <Storyboard x:Name="timeout">
      <DoubleAnimation x:Name="imageTimeout" Duration="0:0:10"/>
    </Storyboard>
  </Canvas.Resources>
        
  <Image x:Name="imageElement"
                Canvas.Left="0" Canvas.Top="0"
                />

        
</Canvas>
