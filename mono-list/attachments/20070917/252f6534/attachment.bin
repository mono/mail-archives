using System;
using System.Data;
using System.Configuration;
using System.Web;
using System.Web.Security;
using System.Web.UI;
using System.Web.UI.WebControls;
using System.Web.UI.WebControls.WebParts;
using System.Web.UI.HtmlControls;

using MySql.Data.MySqlClient;
using System.Data.Common;
using System.Data.SqlClient;

public partial class _Default : System.Web.UI.Page 
{
    public string connectionString;
    public string provider;

    protected void Page_Load(object sender, EventArgs e)
    {
        //
    }



    protected void Page_Init(object sender, EventArgs e)
    {
        connectionString = System.Configuration.ConfigurationManager.ConnectionStrings["MySqlServer"].ConnectionString;
        provider = System.Configuration.ConfigurationManager.ConnectionStrings["MySqlServer"].ProviderName;

        this.SqlStructure.ConnectionString = connectionString;
        this.SqlStructure.ProviderName = provider;
    }

}