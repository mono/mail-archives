<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="moduleprogression">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="weight" />
        <xs:element ref="complete" />
      </xs:sequence>
      <xs:attribute name="type" />
    </xs:complexType>
  </xs:element>
  <xs:element name="allduration">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
      <xs:attribute name="unit" />
    </xs:complexType>
  </xs:element>
  <xs:element name="release">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="manual" />
        <xs:element ref="automatic" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="progression">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="weight" />
        <xs:element ref="release" />
        <xs:element ref="validation" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="plan">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name" />
        <xs:element ref="acronym" />
        <xs:element ref="allduration" />
        <xs:element ref="description" />
        <xs:element ref="designers" />
        <xs:element ref="resource" />
        <xs:element ref="planprogression" />
        <xs:element ref="module" />
      </xs:sequence>
      <xs:attribute name="id" />
    </xs:complexType>
  </xs:element>
  <xs:element name="groupwork">
    <xs:complexType />
  </xs:element>
  <xs:element name="note">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name" />
        <xs:element ref="date" />
        <xs:element ref="description" />
      </xs:sequence>
      <xs:attribute name="type" />
    </xs:complexType>
  </xs:element>
  <xs:element name="planprogression">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="complete" />
      </xs:choice>
      <xs:attribute name="type" />
    </xs:complexType>
  </xs:element>
  <xs:element name="designers">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="designer" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="team">
    <xs:complexType />
  </xs:element>
  <xs:element name="complete">
    <xs:complexType />
  </xs:element>
  <xs:element name="evaluation">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="date">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="designer">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="validation">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="activityfile">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="plan" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="moduleplanification">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="duration" />
        <xs:element ref="period" />
        <xs:element ref="evaluation" />
        <xs:element ref="interaction" />
        <xs:element ref="groupwork" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="description">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="name">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="period">
    <xs:complexType />
  </xs:element>
  <xs:element name="group">
    <xs:complexType />
  </xs:element>
  <xs:element name="datelimit">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="planification">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="duration" />
        <xs:element ref="period" />
        <xs:element ref="evaluation" />
        <xs:element ref="interaction" />
        <xs:element ref="jobs" />
        <xs:element ref="datelimit" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="manual">
    <xs:complexType />
  </xs:element>
  <xs:element name="advice">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="title" />
        <xs:element ref="description" />
        <xs:element ref="resource" />
      </xs:sequence>
      <xs:attribute name="id" />
    </xs:complexType>
  </xs:element>
  <xs:element name="jobs">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="advices">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="advice" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="activity">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name" />
        <xs:element ref="resource" />
        <xs:element ref="role" />
        <xs:element ref="planification" />
        <xs:element ref="progression" />
        <xs:element ref="advices" />
        <xs:element ref="notes" />
      </xs:sequence>
      <xs:attribute name="id" />
    </xs:complexType>
  </xs:element>
  <xs:element name="acronym">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="automatic">
    <xs:complexType />
  </xs:element>
  <xs:element name="duration">
    <xs:complexType />
  </xs:element>
  <xs:element name="notes">
    <xs:complexType>
      <xs:choice>
        <xs:element ref="note" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
  <xs:element name="title">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="role">
    <xs:complexType />
  </xs:element>
  <xs:element name="module">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="name" />
        <xs:element ref="resource" />
        <xs:element ref="moduleplanification" />
        <xs:element ref="moduleprogression" />
        <xs:element ref="notes" />
        <xs:choice minOccurs="0" maxOccurs="unbounded">
          <xs:element ref="activity" />
          <xs:element ref="module" />
        </xs:choice>
      </xs:sequence>
      <xs:attribute name="id" />
    </xs:complexType>
  </xs:element>
  <xs:element name="weight">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
    </xs:complexType>
  </xs:element>
  <xs:element name="interaction">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="team" />
        <xs:element ref="group" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="resource">
    <xs:complexType mixed="true">
      <xs:choice minOccurs="0" maxOccurs="unbounded" />
      <xs:attribute name="type" />
    </xs:complexType>
  </xs:element>
</xs:schema>
