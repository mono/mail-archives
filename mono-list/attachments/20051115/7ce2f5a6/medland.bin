/* 
 * compile as:
 * 	gcc -g -shared -o libmedland.so medland.c
 */

#include <stdio.h>
#include <string.h>

typedef struct {
	unsigned short length;
	char mystring1 [8];
	signed int myint;
	char mystring2 [12];
} mystruct;

void
myfunc (const mystruct *input)
{
	char s1 [9];
	char s2 [13];

	memcpy (s1, input->mystring1, 8);
	s1[8] = '\0';
	memcpy (s2, input->mystring2, 12);
	s2[12] = '\0';

	printf ("unmanaged: input = { .length=%i, .mystring1=\"%s\", \n"
			"\t.myint=%i, .mystring2=\"%s\"\n", input->length, s1, 
			input->myint, s2);
}

