Index: src/Cocoa/MutableAttributedString.cs
===================================================================
--- src/Cocoa/MutableAttributedString.cs	(revision 0)
+++ src/Cocoa/MutableAttributedString.cs	(revision 0)
@@ -0,0 +1,75 @@
+using System;
+using System.Runtime.InteropServices;
+using Cocoa;
+
+namespace Cocoa {
+	public class MutableAttributedString : AttributedString {
+		private static string ObjectiveCName = "NSMutableAttributedString";                                                                                      
+
+		static MutableAttributedString () {
+			NativeClasses [typeof (MutableAttributedString)] = Native.RegisterClass (typeof (MutableAttributedString)); 
+		}
+
+		public MutableAttributedString (IntPtr native_object) : base (native_object) {}
+
+		public void AddAttribute (string name, Object val, Range range) {
+			ObjCMessaging.objc_msgSend (NativeObject, "addAttribute:value:range:", typeof (void), typeof (IntPtr), new Cocoa.String (name).NativeObject, typeof (IntPtr), (val == null ? IntPtr.Zero : val.NativeObject), typeof (Range), range);
+		}
+
+		public void AddAttributes (Dictionary val, Range range) {
+			ObjCMessaging.objc_msgSend (NativeObject, "addAttributes:range:", typeof (void), typeof (IntPtr), (val == null ? IntPtr.Zero : val.NativeObject), typeof (Range), range);
+		}
+
+		public void AppendAttributedString (AttributedString str) {
+			ObjCMessaging.objc_msgSend (NativeObject, "appendAttributedString:", typeof (void), typeof (IntPtr), (str == null ? IntPtr.Zero : str.NativeObject));
+		}
+
+		public void BeginEditing () {
+			ObjCMessaging.objc_msgSend (NativeObject, "beginEditing", typeof (void));
+		}
+
+		public void DeleteCharactersInRange (Range rng) {
+			ObjCMessaging.objc_msgSend (NativeObject, "deleteCharactersInRange:", typeof (void), typeof (Range), rng);
+		}
+
+		public void EndEditing () {
+			ObjCMessaging.objc_msgSend (NativeObject, "endEditing", typeof (void));
+		}
+
