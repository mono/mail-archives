#include <stdlib.h>
#include <unistd.h>
#include <stdio.h>

int main (int argc, char **argv)
{
	if (argc < 2)
		printf ("Usage: foo <integer>\n");
	else
	{
		int num = atoi (argv[1]);
		int i;
		
		for (i = 0; i < num; i++)
		{
			printf ("Iteration #(%i)\n", i+1);
			sleep (1);
		}
	}
	printf ("Program finished.\n");

	return 0;
}