using System;
using System.Drawing;

class X {

	static void Main ()
	{
		Point [] points = new Point [] {
			new Point (56,276),
			new Point (55,274),
			new Point (54,269),
			new Point (59,237),
			new Point (70,213),
			new Point (81,206),
			new Point (95,211),
			new Point (137,248),
			new Point (176,260),
			new Point (219,248),
			new Point (251,220),
			new Point (277,161),
			new Point (283,92) };

		Bitmap b = new Bitmap (300, 300);
		Graphics g = Graphics.FromImage (b);
		Pen p = new Pen (Color.Black, 3);

		g.FillRectangle (Brushes.White, new Rectangle (0, 0, 300, 300));
		g.DrawCurve (p, points, 0, points.Length, 1.0f);
		b.Save ("curve.png");
	}
}