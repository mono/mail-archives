#include <stdio.h>
#include <sys/time.h>
#include <sys/types.h>
#include <unistd.h>
#include <mono/io-layer/io-layer.h>

int main(int argc, char **argv)
{
	fd_set z;
	int sock;
	struct timeval tv;

	sock=socket(PF_UNIX, SOCK_STREAM, AF_UNIX);
	FD_ZERO (&z);
	FD_SET (sock, &z);

	tv.tv_sec = 10;
	tv.tv_usec = 0;

	if (select (sock + 1, &z, NULL, NULL, &tv)) {
		printf("selected\n");
	}

	exit(0);
}

