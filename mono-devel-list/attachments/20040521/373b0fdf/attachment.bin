using System;

class T {
	
	static void Main ()
	{
		for (int i = 1; i <= 1024; i *= 2) {
			Console.Write ("{0,4}  | ", i);
			Test (i);
			Console.WriteLine ();
		}
	}
	
	static void Test (int n) {
		
		{
			string a = new string ('a', n);
			string b = new string ('a', n);
			int t0 = Environment.TickCount;
			for (int i = 0; i < 5000000; i ++)
				String.Equals (a, b);
			Console.Write ("{0,6} ", Environment.TickCount - t0);
		}
		
		{
			string a = new string ('a', n);
			string b = new string ('b', n);
			int t0 = Environment.TickCount;
			for (int i = 0; i < 5000000; i ++)
				String.Equals (a, b);
			Console.Write ("{0,6} ", Environment.TickCount - t0);
		}
		
		{
			string a = new string ('a', n + 1);
			string b = new string ('a', n);
			int t0 = Environment.TickCount;
			for (int i = 0; i < 5000000; i ++)
				String.Equals (a, b);
			Console.Write ("{0,6} ", Environment.TickCount - t0);
		}
		
		{
			string a = new string ('a', n);
			string b = a;
			int t0 = Environment.TickCount;
			for (int i = 0; i < 5000000; i ++)
				String.Equals (a, b);
			Console.Write ("{0,6} ", Environment.TickCount - t0);
		}
	}
}