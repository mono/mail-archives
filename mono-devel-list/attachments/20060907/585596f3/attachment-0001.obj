<%@ Page Language="C#" %>

<script runat="server">
    
    class MyEnumerable : IEnumerable
    {
        public IEnumerator GetEnumerator ()
        {
            return new MyEnumerator();
        }
    }
    class MyEnumerator : IEnumerator
    {
        int i;

        public object Current
        {
            get { return i.ToString(); }
        }

        public bool MoveNext ()
        {
            if (i==100)
                return false;
            i ++;
            return true;
        }

        public void Reset ()
        {
            i=0;
        }

    }
    
    class GridViewPoker : GridView
    {
        public DataSourceView DoGetData ()
        {
            return GetData ();
        }
    }

    GridViewPoker gvp;
    
    protected override void OnLoad (EventArgs e)
    {
        //GridView1.DataSource = new MyEnumerable ();
        //GridView1.DataBind ();
        GridView1.DataSourceID = "SqlDataSource1";
        gvp = new GridViewPoker ();
        gvp.ID = "GridView2";
        gvp.AllowPaging = true;
        gvp.AutoGenerateColumns = true;
        gvp.PageSize = 2;
        gvp.DataSourceID = "SqlDataSource1";
        this.Form.Controls.Add (gvp);
    }

    protected override void OnPreRenderComplete (EventArgs e)
    {
        base.OnPreRenderComplete (e);
        DataSourceView dsv = gvp.DoGetData ();
        Response.Write (dsv.GetType ().ToString ());
        Response.Write ("<br/>\n");
        dsv.Select (new DataSourceSelectArguments (), DataSourceViewSelectCallback);
    }

    public void DataSourceViewSelectCallback (IEnumerable data)
    {
        Response.Write (data.GetType ().ToString ());
    }

</script>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" >
<head runat="server">
    <title>title</title>
</head>
<body>
    <form id="form1" runat="server" method ="post">
    <div>
        <asp:GridView ID="GridView1" runat="server" AllowPaging="True"
            PageSize="2"
            AutoGenerateColumns="true"
            EmptyDataText="There are no data records to display."
             >
        </asp:GridView>
        <asp:SqlDataSource ID="SqlDataSource1" runat="server" ConnectionString="<%$ ConnectionStrings:PersonalConnectionString1 %>"
            DeleteCommand="DELETE FROM [Photos] WHERE [PhotoID] = @PhotoID" InsertCommand="INSERT INTO [Photos] ([AlbumID], [Caption]) VALUES (@AlbumID, @Caption)"
            ProviderName="<%$ ConnectionStrings:PersonalConnectionString1.ProviderName %>"
            SelectCommand="SELECT [PhotoID], [AlbumID], [Caption], [Caption], [BytesOriginal], [BytesFull], [BytesPoster], [BytesThumb] FROM [Photos];SELECT [PhotoID], [AlbumID], [Caption], [BytesOriginal], [BytesFull], [BytesPoster], [BytesThumb] FROM [Photos]"
            UpdateCommand="UPDATE [Photos] SET [AlbumID] = @AlbumID, [Caption] = @Caption WHERE [PhotoID] = @PhotoID">
            <InsertParameters>
                <asp:Parameter Name="AlbumID" Type="Int32" />
                <asp:Parameter Name="Caption" Type="String" />
            </InsertParameters>
            <UpdateParameters>
                <asp:Parameter Name="AlbumID" Type="Int32" />
                <asp:Parameter Name="Caption" Type="String" />
                <asp:Parameter Name="PhotoID" Type="Int32" />
            </UpdateParameters>
            <DeleteParameters>
                <asp:Parameter Name="PhotoID" Type="Int32" />
            </DeleteParameters>
        </asp:SqlDataSource>
        
                
    </div>
    </form>
</body>
</html>
