using System;
using System.Xml;
using System.Xml.Serialization;

public class Test
{
	// To see IsNullabe result, switch these attributes.
//	[XmlElement (IsNullable=true)]
	[XmlAttribute]
	public XmlQualifiedName qname;

	public static void Main (string [] args)
	{
		XmlQualifiedName emptyQN = new XmlQualifiedName ("", "");
		Console.WriteLine (emptyQN == XmlQualifiedName.Empty);
		Console.WriteLine (emptyQN == null);
		XmlSerializer ser = new XmlSerializer (typeof (Test));
		Test t = new Test ();
		t.qname = emptyQN;
		ser.Serialize (new XmlTextWriter (Console.Out), t);
	}
}
