	public class MainClass
	{
		static void Main (string[] args)
		{
			while (true)
			{
				byte[] val = new byte[] { 5,6,2,1,6,4,3,6,8,6,4,2,4,6};
				long start = Environment.TickCount;
				for (long i=0; i < 50000000; i++)
