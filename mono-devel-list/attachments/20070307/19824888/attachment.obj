using System;

public class Global:System.Web.HttpApplication
{
	private static Random random;

	static Global()
	{
		Global.random = new Random();
	}

	public static int rand1()
	{
		lock(Global.random)
		{
			return Global.random.Next(0, 9);
		}
	}

	public static int rand2
	{
		get
		{
			lock(Global.random)
			{
				return Global.random.Next(0, 9);
			}
		}
	}

}//Class "Global".
