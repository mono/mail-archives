<nlog internalLogToConsole="false" internalLogLevel="Warn">
    <targets>
	<target name="ilog" type="MethodCall"
	    className="Foo.Test, logtest" methodName="LogMessage">
	    <parameter type="System.String"
		layout="${callsite}: ${message}" />
	</target>
    </targets>

    <rules>
	<logger name="*" minlevel="Debug" appendTo="ilog" />
    </rules>
</nlog>
