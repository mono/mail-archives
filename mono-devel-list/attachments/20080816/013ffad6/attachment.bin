using System;

static class ProgramLoop
{
	static void Main(string[] Arguments)
	{
		for(int i = 0 ; i < 2000000000 ; i += 10)
		{
			HighestSetBit (i%64);
		}
	}

        private static int HighestSetBit (int i)
        {
            int count = 0;
            while(i >> count != 0)
                count++;
            return count;
        }
}