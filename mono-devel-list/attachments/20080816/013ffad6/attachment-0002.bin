using System;

static class ProgramOptimized
{
	static void Main(string[] Arguments)
	{
		for(int i = 1 ; i < 2000000000 ; i += 10)
			if (MostSignificantBit (i) != (1 << HighestSetBit (i) - 1))
				throw new Exception ("Bug using i = " + i);

		if (MostSignificantBit (int.MaxValue) != (1 << HighestSetBit (int.MaxValue) - 1))
			throw new Exception ("Bug using i = " + int.MaxValue);
	}

	static private int MostSignificantBit(int x)
	{
		x |= (x >> 1);
		x |= (x >> 2);
		x |= (x >> 4);
		x |= (x >> 8);
		x |= (x >> 16);
		return(x & ~(x >> 1));
	}

        private static int HighestSetBit (int i)
        {
            int count = 0;
            while(i >> count != 0)
                count++;
            return count;
        }
}