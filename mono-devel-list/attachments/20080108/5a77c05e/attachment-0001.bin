using System;
using System.Diagnostics;

class Test {
	static void Main(string[] args) {
		object o = "TestString";
		string s;
		string s1;
		
		Stopwatch sw = new Stopwatch();
		for(int runs = 0; runs < 10; runs++) { 
			sw.Reset();
			sw.Start();
			for(int i = 0; i < 100000000; i++) {
				s = o as string;
				s1 = s;
			}
			sw.Stop();
			Console.WriteLine("'as' cast:     {0}", sw.ElapsedMilliseconds);
	
			sw.Reset();
			sw.Start();	
			for(int i = 0; i < 100000000; i++) {
				s = (string)o;
				s1 = s;
			}
			sw.Stop();
			Console.WriteLine("explicit cast: {0}", sw.ElapsedMilliseconds);
			Console.WriteLine();
		}		
	}
}