using System;

public class test {
    static void Main() {
        string[][] times;
        times = new string[4][];
        for (int i = 0; i < times.Length; i++) {
            times[i] = new string[5];
            for (int j = 0; j < times[i].Length; j++) {
					times[i][j] = "field " + i + " - " + j;
            }
        }
        for (int i = 0; i < times.Length; i++) {
            for (int j = 0; j < times[i].Length; j++) {
                Console.WriteLine("{0,-5} {1,-5}: {2}", i, j, times[i][j]);
            }
        }
    }
}
