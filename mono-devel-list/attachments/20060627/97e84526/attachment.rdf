<%@ Page Language="C#" %>
<%@ Import Namespace="System.Globalization" %>
<%
byte[] data = Encoding.ASCII.GetBytes("This is some binary data in fact.");

Response.Clear();
Response.ContentType = "application/octet-stream";
// When sending text files you probably want to set the correct charset
Response.Charset = null;
Response.AppendHeader("Content-Disposition", "attachment; filename=\"filename.bin\"");
Response.AppendHeader("Content-Length", data.Length.ToString(CultureInfo.CurrentCulture));
Response.BinaryWrite(data);
Response.End();
%>