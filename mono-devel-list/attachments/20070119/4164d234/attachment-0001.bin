#include <stdint.h>
#include <stdlib.h>
#include <string.h>

struct device_entry_struct {
	const char *name;
	uint16_t vendor_id;
	uint16_t product_id;
	uint32_t device_flags;
};

static struct device_entry_struct devices[] = {
	{ "A", 1, 2, 3 },
	{ "B", 4, 5, 6 },
	{ "C", 7, 8, 9 },
};

void
Get_Supported_Devices (struct device_entry_struct **list, int *count)
{
	*count = sizeof(devices) / sizeof(devices[0]);
	*list  = malloc (sizeof(devices));

	memcpy (*list, devices, sizeof(devices));
}

