#include <stdio.h>

int
main (void) {
  int v = 0;
  int a = 0;
  printf ("/* precomputed table for value*255 divised by alpha */\n"
	  "const BYTE pre_multiplied_table_rev[256][256] = {\n");

  for (a = 0; a < 256; ++a) {
    printf ("\t/* %03d */\t {", a);
    for (v = 0; v <= a; ++v) {
      if (v != 0)
	printf (",");
      printf (" %3d", a == 0 ? 0 : v*255/a);
    }
    printf (" }");
    if (a < 255)
      printf (",");

    printf ("\n");
  }
  printf ("};\n");

  return 0;
}
