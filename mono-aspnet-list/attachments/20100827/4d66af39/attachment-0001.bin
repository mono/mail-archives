using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Web;
using System.Web.Hosting;

namespace Test
{
    class Program
    {
        static void Main(string[] args)
        {
            Host host = (Host) ApplicationHost.CreateApplicationHost(typeof(Host), "/", Environment.CurrentDirectory);
            host.ProcessRequest(args[0]);
        }
    }

    class Host : MarshalByRefObject
    {
        public void ProcessRequest(string page)
        {
            MyWorkerRequest worker = new MyWorkerRequest(page);
            HttpRuntime.ProcessRequest(worker);
        }
    }

    class MyWorkerRequest : SimpleWorkerRequest
    {
        public MyWorkerRequest(string page) : base (page, null, Console.Out)
        {
        }

        public override void SetEndOfSendNotification(HttpWorkerRequest.EndOfSendNotification callback, object extraData)
        {
            Console.WriteLine("I've been called. Data is {0}", extraData);
            Console.WriteLine(Environment.StackTrace);
        }
    }
}

